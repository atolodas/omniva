{
  "name": "bigbank/omniva",
  "description": "Library to interface with Omniva",
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {
      "Bigbank\\Omniva\\": "src/"
    }
  },
  "require": {
    "ext-xml": "*",
    "ext-curl": "*",
    "ext-soap": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "php": ">=5.6",
    "league/container": "~2.0"
  },
  "require-dev": {
    "wsdl2phpgenerator/wsdl2phpgenerator": "~3"
  },
  "scripts": {
    "post-update-cmd": "bin/wsdl_generator",
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
